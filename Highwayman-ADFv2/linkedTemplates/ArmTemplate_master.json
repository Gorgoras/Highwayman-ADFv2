{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "highwayman-adfv2"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStore1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureDataLakeStore1'"
		},
		"Blob_brahminstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blob_brahminstorage'"
		},
		"FS_Notebook_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FS_Notebook'"
		},
		"Caravan2_brahmin_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"Caravan2_brahmin_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "caravan2"
		},
		"CsvSales_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "*.csv"
		},
		"LakeSales_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"LakeSales_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "Sales"
		},
		"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://lakemead1.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "92edc83e-82bc-46a3-ab79-2fc09b752cd0"
		},
		"AzureDataLakeStore1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "ca1d08b0-9543-4bd9-8718-42becddc7786"
		},
		"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "fc2b6f69-07e8-4361-a14c-926b7e50d69e"
		},
		"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "Wasteland"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://chargecard764Id.vault.azure.net/"
		},
		"FS_Notebook_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "DESKTOP-DIIS9C2\\marti"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStore1_servicePrincipalKey": {
						"value": "[parameters('AzureDataLakeStore1_servicePrincipalKey')]"
					},
					"Blob_brahminstorage_connectionString": {
						"value": "[parameters('Blob_brahminstorage_connectionString')]"
					},
					"FS_Notebook_password": {
						"value": "[parameters('FS_Notebook_password')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_resourceGroupName')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"Caravan2_brahmin_properties_typeProperties_fileName": {
						"value": "[parameters('Caravan2_brahmin_properties_typeProperties_fileName')]"
					},
					"Caravan2_brahmin_properties_typeProperties_folderPath": {
						"value": "[parameters('Caravan2_brahmin_properties_typeProperties_folderPath')]"
					},
					"LakeSales_properties_typeProperties_fileName": {
						"value": "[parameters('LakeSales_properties_typeProperties_fileName')]"
					},
					"LakeSales_properties_typeProperties_folderPath": {
						"value": "[parameters('LakeSales_properties_typeProperties_folderPath')]"
					},
					"FS_Notebook_properties_typeProperties_userId": {
						"value": "[parameters('FS_Notebook_properties_typeProperties_userId')]"
					},
					"CsvSales_properties_typeProperties_fileName": {
						"value": "[parameters('CsvSales_properties_typeProperties_fileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}